  <DataSets>
    <DataSet Name="Performance_Individu_Datsi">
      <Query>
        <DataSourceName>Performance_Individu_Datsi</DataSourceName>
        <CommandText>SELECT users.name as username, kategoritugas.alternate_name as task,
COUNT (CASE WHEN todolist.statusAktivitas=1 or todolist.statusAktivitas=2 THEN 1 ELSE null END ) AS jumlah_task, month(todolist.tanggal) as bulan, year(todolist.tanggal) as tahun
      FROM users 
        CROSS JOIN kategoritugas 
        LEFT JOIN todolist ON users.name = todolist.username
          AND kategoritugas.id = todolist.id_kategori
      group BY users.name, kategoritugas.alternate_name, month(todolist.tanggal), year(todolist.tanggal)</CommandText>
      </Query>
      <Fields>
        <Field Name="username">
          <DataField>username</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="task">
          <DataField>task</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="jumlah_task">
          <DataField>jumlah_task</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="bulan">
          <DataField>bulan</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="tahun">
          <DataField>tahun</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
